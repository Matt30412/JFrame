CREATE TABLE `utente` (
	`Nome` VARCHAR(64) NOT NULL COLLATE 'utf8mb4_general_ci',
	`Cognome` VARCHAR(64) NOT NULL COLLATE 'utf8mb4_general_ci',
	`Email` VARCHAR(64) NOT NULL COLLATE 'utf8mb4_general_ci',
	`Password` VARCHAR(64) NOT NULL COLLATE 'utf8mb4_general_ci',
	`CV` VARCHAR(64) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`cv2` MEDIUMBLOB NULL DEFAULT NULL,
	`Skill` INT(11) NULL DEFAULT NULL,
	`DataI` DATE NULL DEFAULT NULL,
	`DataF` DATE NULL DEFAULT NULL,
	`Controllo` INT(4) NULL DEFAULT NULL,
	PRIMARY KEY (`Email`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
;
